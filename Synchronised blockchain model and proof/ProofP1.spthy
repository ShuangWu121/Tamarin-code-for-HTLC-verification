   ),
   !PK( B, pkB4 )
   ]
  --[ Bob_receive( CommitTxAlice ) ]->
   [
   Out( <CommitTxAlice, 
         <hsk, 
          sign(<'BC1', CommitTxAlice, <pkA1, timelock_A, hash, pk(ltkB3)>, 
                pkB4>,
               ltkB3)
         >, 
